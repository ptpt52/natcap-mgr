#!/bin/sh
PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games:/usr/local/games

ipset destroy vclist
ipset create vclist hash:mac
cat $0 | grep ^[a-fA-F0-9][a-fA-F0-9]: | sort | uniq | while read line; do
	ipset add vclist $line
done

exit 0

